<!DOCTYPE html>
<html>
<head>
  <title>Ticketer</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= flash_messages %>
  <% if user_signed_in? %>
    <%= render 'layouts/topnavbar' %>
    <%= render 'layouts/sidenavbar' %>
  <% end %>
  <%= yield %>
</body>
</html>